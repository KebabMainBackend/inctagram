{
  "name": "inctagram",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/inctagram/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/inctagram/test/jest-e2e.json",
    "build:inctagram": "nest build inctagram",
    "start:inctagram": "node dist/apps/inctagram/main.js",
    "start:dev:inctagram": "nest start inctagram --watch",
    "build:files": "nest build files",
    "start:files": "node dist/apps/files/main.js",
    "start:dev:files": "nest start files --watch",
    "build:payments": "nest build payments",
    "start:payments": "node dist/apps/payments/main.js",
    "start:dev:payments": "nest start payments --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/cqrs": "10.2.6",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/throttler": "5.1.1",
    "@prisma/client": "5.9.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "date-fns": "3.3.1",
    "google-auth-library": "^9.6.3",
    "image-size": "^1.1.1",
    "mongoose": "^8.2.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemailer": "6.9.9",
    "passport": "0.7.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "5.9.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sharp": "0.32.6",
    "stripe": "^14.22.0",
    "wrap-ansi": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.6",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-github": "^1.1.12",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
